color7  = #c0c0c0 # base05 - White
color8  = #545454 # base03 - Bright Black
color9  = #fc5454 # base08 - Bright Red
color10 = #54fc54 # base0B - Bright Green
color11 = #fcfc54 # base0A - Bright Yellow
color12 = #5454fc # base0D - Bright Blue
color13 = #fc54fc # base0E - Bright Magenta
color14 = #54fcfc # base0C - Bright Cyan
