<%# vim: set filetype=eruby.gpg : -%>
# <%= variables.figManaged %>

allow-preset-passphrase

# After 30 days of inactivity, require passphrase again
# (default is 600 seconds).
default-cache-ttl 1209600

# 30 days after passphrase was entered, require passphrase again
# (default is 7200 seconds).
max-cache-ttl 1209600

pinentry-program <%= variables.pinentryProgram %>
